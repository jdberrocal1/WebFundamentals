            <div class="col-lg-5">
                <table id="personalInfo">
                    <thead>
                        <tr>
                            <th>
                                <span>Name</span>
                            </th>
                            <th>
                                <span>Email</span>
                            </th>
                            <th>
                                <span>Phone</span>
                            </th>
                        </tr>
